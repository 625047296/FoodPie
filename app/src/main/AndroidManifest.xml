<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dllo.foodpie">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name=".netrequest.MyApp"
        android:theme="@style/CustomTitle">
        <activity android:name=".welcomeactivity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".mainactivity.MainActivity"></activity>
        <activity android:name=".setactivity.SetActivity"></activity>
        <activity android:name=".enter.EnterActivity"></activity>
    </application>

</manifest>